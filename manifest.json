{
  "manifest_version": 2,
  "name": "Social Launcher",
  "version": "1.0.5",
  "description": "Quick Launce To your socials",
  "icons": { 
         "128": "images/social_launcher_logo.png" 
  },
  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "author": "Hasanuzzaman",
  "optional_permissions": ["tabs"],
  "permissions": ["tabs", "topSites", "storage"],
  "short_name": "S Launch"
}
